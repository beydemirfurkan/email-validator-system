{
  "name": "email-validator-api",
  "version": "1.0.0",
  "description": "Professional email validation API with batch processing, CSV support, and detailed statistics",
  "main": "dist/app.js",
  "keywords": [
    "email",
    "validation",
    "api",
    "bulk",
    "csv",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:load": "artillery run tests/load-test.artillery.yml",
    "test:stress": "node tests/stress-test.js",
    "test:concurrent": "jest --testPathPattern=concurrent-users.test.ts --runInBand",
    "test:all": "npm run test && npm run test:load",
    "debug": "node --inspect-brk=0.0.0.0:9229 dist/app.js",
    "profile": "node --prof dist/app.js",
    "lint": "echo 'ESLint not configured'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@upstash/redis": "^1.35.3",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.6.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "disposable-email-domains": "^1.0.62",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.38.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "punycode.js": "^2.3.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "artillery": "^2.0.24",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "drizzle-kit": "^0.30.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
